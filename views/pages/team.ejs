<!DOCTYPE html>
<html lang="en">

<head>
  <% include ../partials/head.ejs %>
    <title>Team | HackSoc</title>
    <link rel="stylesheet" type="text/css" href="css/team.css" />
</head>

<body>
  <div id="container">
    <% include ../partials/navbar.ejs %>
      <div id="body">
          <section id="team">
              <h1>Team</h1>
              <div class="members">
                <% team.forEach(member => { %>
                  <div class="member" style="background-image: url('images/team/<%= member.id %>.small.jpg')">
                    <div>
                      <h2><%= member.name %></h2>
                      <p><%= member.description %></p>
                    </div>
                  </div>
                <% }) %>
              </div>
              <div class="separator"></div>
              <h1>Hall of Fame</h1>
              <div class="members">
                <% hallOfFame.forEach(member => { %>
                  <div class="member" style="background-image: url('images/team/<%= member.id %>.small.jpg')">
                    <div>
                      <h2><%= member.name %></h2>
                      <p><%= member.description %></p>
                    </div>
                  </div>
                <% }) %>
              </div>
            </section>
      </div>
      <% include ../partials/footer.ejs %>
  </div>
</body>

</html>